config_version=5

[application]

config/name="AI-RTS Cooperative Game"
config/version="0.1.0"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

# Shared components
GameEnums="res://shared/types/game_enums.gd"
GameConstants="res://shared/constants/game_constants.gd"
NetworkMessages="res://shared/types/network_messages.gd"

# Client-side autoloads
ClientDisplayManager="res://scripts/client/client_display_manager.gd"
EventBus="res://scripts/autoload/event_bus.gd"
Logger="res://scripts/utils/logger.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="AI-RTS Cooperative Game"

